<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">Course Number</th>
      <th scope="col">Edit</th>
      <th scope="col">Delete</th>
    </tr>
  </thead>

  <tbody>
    <% i = 1 %>
    <% @my_application.each do | application | %>
    <tr>

      <th scope="row"><%= i %> </th>
      <td><%= application.course_number %></td>
      <td><%=link_to "Edit", edit_application_path(id: application.id), :class=>"btn btn-info" %></td>
      <td><%=link_to "Delete", application_path(id: application.id), method: :delete, :class=>"btn btn-danger" %></td>

    </tr>
    <%i = i + 1%>
    <%end%>
  </tbody>

</table>

<div class="d-flex flex-row">



</div>